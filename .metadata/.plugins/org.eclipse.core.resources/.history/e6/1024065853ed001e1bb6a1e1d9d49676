package br.com.senaisp.bauru.alex.licaoo7secaoexemplos;

public class Terminal {
	private static int nId = 0;
	private CategoriaPremio categoria1;
	private CategoriaPremio categoria2;
	private CategoriaPremio categoria3;
	private int numeroTeminal;
	
	public Terminal () {
		numeroTeminal = ++ nId;
		categoria1 = new CategoriaPremio("gold",25);
		categoria2 = new CategoriaPremio("silver",50);
		categoria3 = new CategoriaPremio("bronze", 75);
	}
	//Getters e Setters
	public CategoriaPremio getCategoria1() {
		return categoria1;
	}

	public CategoriaPremio getCategoria2() {
		return categoria2;
	}

	public CategoriaPremio getCategoria3() {
		return categoria3;
	}

	public int getNumeroTeminal() {
		return numeroTeminal;
	}
	//metodos
	
}
