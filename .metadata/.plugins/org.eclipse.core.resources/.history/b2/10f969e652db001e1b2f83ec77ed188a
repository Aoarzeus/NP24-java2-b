package br.com.senaisp.bauru.alex.secao02;

public class Text01 {
	/**
	 * documentação 
	 * @param args
	 */
    public static void main(String[] args) {
    	//linha
        System.out.println("   /\\         /\\   ");
        /*
         *  duas linhas 
         */
        System.out.println("  /  \\_______/  \\  ");
        System.out.println(" /               \\ ");
        System.out.println("(  /\\         /\\  )");
        System.out.println("====     V     ====");
        System.out.println("======(__|__)======");
        {
        System.out.println("  (             )  ");
        System.out.println("   (___________)   ");
        }
        }    
}
